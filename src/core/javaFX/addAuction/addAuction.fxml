             <ImageView fx:id="imgviewPicture4" fitHeight="156.0" fitWidth="225.0" layoutX="750.0" layoutY="50.0" onMouseClicked="#addImage" pickOnBounds="true" preserveRatio="true">
                 <cursor>
                     <Cursor fx:constant="HAND" />
                 </cursor>
            </ImageView>
             
             <JFXTextField fx:id="txtTitle" alignment="CENTER" focusColor="#747e8c" layoutX="25.0" layoutY="244.0" prefHeight="35.0" prefWidth="950.0" promptText="title" style="-fx-text-fill: #a6b5c9; -fx-prompt-text-fill: #788599; -fx-background-color: #3d4857;" unFocusColor="#242e3b">
                 <font>
                     <Font name="Segoe UI Light" size="18.0" />
                 </font>
             </JFXTextField>

             <JFXTextArea fx:id="txtDescription" focusColor="#788599" layoutX="25.0" layoutY="320.0" prefHeight="105.0" prefWidth="950.0" promptText="description" style="-fx-text-fill: #a6b5c9; -fx-prompt-text-fill: #788599; -fx-background-color: transparent;" unFocusColor="#242e3b">
