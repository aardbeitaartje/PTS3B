            </JFXTextField>

             <JFXTextField fx:id="txtEndTime" focusColor="#788599" layoutX="25.0" layoutY="605.0" prefHeight="35.0" prefWidth="250.0" promptText="end time (HH:MM)" style="-fx-text-fill: #a6b5c9; -fx-prompt-text-fill: #788599; -fx-background-color: #3d4857;" unFocusColor="#242e3b">
