             <ImageView fx:id="imgviewPicture3" fitHeight="156.0" fitWidth="225.0" layoutX="508.3" layoutY="50.0" onMouseClicked="#addImage" pickOnBounds="true" preserveRatio="true">
                 <cursor>
                     <Cursor fx:constant="HAND" />
