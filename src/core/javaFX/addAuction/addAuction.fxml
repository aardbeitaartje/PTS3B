            <JFXTextField fx:id="txtIncrementation" focusColor="#788599" layoutX="725.0" layoutY="605.0" prefHeight="35.0" prefWidth="250.0" promptText="minimal incrementation" style="-fx-text-fill: #a6b5c9; -fx-prompt-text-fill: #788599; -fx-background-color: #3d4857;" unFocusColor="#242e3b">
                <font>
                    <Font name="Segoe UI Light" size="18.0" />
                </font>
