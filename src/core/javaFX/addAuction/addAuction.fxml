            <JFXButton layoutX="350.0" layoutY="675.0" onAction="#createAuction" prefHeight="45.0" prefWidth="300.0" style="-fx-background-color: f2aa78;" text="Create auction" textFill="#824923">
                <font>
                    <Font name="Segoe UI Light" size="14.0" />
                </font>
                <cursor>
                    <Cursor fx:constant="HAND" />
                </cursor>
            </JFXButton>
            <Label layoutX="375.0" layoutY="438.0" text="minimal sale price:" textFill="#788599">
