            <JFXTextField fx:id="txtStartDate" focusColor="#788599" layoutX="375.0" layoutY="530.0" prefHeight="35.0" prefWidth="250.0" promptText="start date (DD/MM/YYYY)" style="-fx-text-fill: #a6b5c9; -fx-prompt-text-fill: #788599; -fx-background-color: #3d4857;" unFocusColor="#242e3b">
