         <children>
             <ImageView fx:id="imgviewPicture1" fitHeight="156.0" fitWidth="225.0" layoutX="25.0" layoutY="50.0" onMouseClicked="#addImage" pickOnBounds="true" preserveRatio="true">
                 <cursor>
                     <Cursor fx:constant="HAND" />
                 </cursor>
             </ImageView>
             <ImageView fx:id="imgviewPicture2" fitHeight="156.0" fitWidth="225.0" layoutX="267.3" layoutY="50.0" onMouseClicked="#addImage" pickOnBounds="true" preserveRatio="true">
                 <cursor>
                     <Cursor fx:constant="HAND" />
                 </cursor>
